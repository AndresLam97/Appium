<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression" parallel="tests" thread-count="3">
    <test name="Test Login Screen With Data Provider" >
        <parameter name="udid" value="emulator-5556"/>
        <parameter name="systemPort" value="6000"/>
        <parameter name="platformName" value="android"/>
        <classes>
            <class name="Exercises.LessonTwentyOne.TestWithDataProvider" />
        </classes>
    </test>
    <test name="Test Webview">
        <parameter name="udid" value="emulator-5554"/>
        <parameter name="systemPort" value="6001"/>
        <parameter name="platformName" value="android"/>
        <classes>
            <class name="Exercises.LessonTwentyOne.TestWebView"/>
        </classes>
    </test>

    <test name="Test Webview On iOS">
        <parameter name="udid" value="iPhone 14"/>
        <parameter name="systemPort" value="6002"/>
        <parameter name="platformName" value="ios"/>
        <parameter name="platformVersion" value="16.1"/>
        <classes>
            <class name="Exercises.LessonTwentyOne.TestWebView"/>
        </classes>
    </test>

</suite>
